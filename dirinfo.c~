#include <stdio.h>
#include <stdlib.h>
#include <dirent.h>
#include <sys/types.h>

int main() {
  char * dirPath = ".";
  DIR * dirStream = oepndir(dirPath);
  struct dirent *  cur = readdir(dirStream);
  int size = 0;
  while (cur != NULL) {
    size += 1;
    printf("%s", cur.d_name);
  }
  closedir(dirStream);
  return 0;
}

